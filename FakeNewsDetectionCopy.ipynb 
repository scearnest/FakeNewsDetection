        "#Actual clustering\n",
        "x_1 = kmeans.fit_predict(k)\n",
        "\n",
        "fig, ax = plt.subplots()\n",
        "for i in range(0, len(x_1)):\n",
        "    if x_1[i] == 1:\n",
        "      color = 'red'\n",
        "      label = 'True'\n",
        "    else:\n",
        "      color = 'blue'\n",
        "      label = 'False'\n",
        "    ax.scatter(k[i,0], k[i,1], c = color, s = 25,alpha=0.25)\n",
