      "source": [
        "#Upload datasets and create true column\n",
        "df_fake = pd.read_csv('Fake.csv')\n",
        "df_true = pd.read_csv('True.csv')\n",
        "df_fake['true'] = 0\n",
        "df_true['true'] = 1\n",
        "df = pd.concat([df_fake, df_true])\n",
        "df"
      ],
